{
    "name": "xemlock/zf1-module",
    "type": "library",
    "description": "Adds support for running ZF1 application in ZF2",
    "keywords": ["zf2", "zf1", "module"],
    "homepage": "https://github.com/xemlock/Zf1Module",
    "license": "MIT",
    "authors": [
        {
            "name": "xemlock",
            "email": "xemlock@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "zendframework/zend-cache": "~2.2",
        "zendframework/zend-config": "~2.2",
        "zendframework/zend-console": "~2.2",
        "zendframework/zend-di": "~2.2",
        "zendframework/zend-http": "~2.2",
        "zendframework/zend-i18n": "~2.2",
        "zendframework/zend-loader": "~2.2",
        "zendframework/zend-log": "~2.2",
        "zendframework/zend-modulemanager": "~2.2",
        "zendframework/zend-mvc": "~2.2",
        "zendframework/zend-navigation": "~2.2",
        "zendframework/zend-serializer": "~2.2",
        "zendframework/zend-view": "~2.2",
        "zendframework/zendframework1": "~1.12"
    },
    "autoload": {
        "psr-4": {
            "Zf1Module\\": "src/"
        },
        "classmap": [
            "Module.php"
        ]
    }
}
